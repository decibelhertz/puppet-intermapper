<%- } -%>
<%- if $limitas { -%>
 LimitAS=<%= $limitas %>
<%- } -%>
<%- if $limitnproc { -%>
 LimitNPROC=<%= $limitnproc %>
<%- } -%>
<%- if $limitmemlock { -%>
